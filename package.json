{
  "name": "mf-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf /tmp/.cache/",
    "dev:ui": "yarn pre-build && DEBUG=true next dev",
    "dev": "yarn load-env-dev concurrently \"yarn dev:ui\"",
    "build": "yarn load-env-dev npm run pre-build && next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "pre-build": "yarn load-env-dev ts-node ./scripts/prebuild.ts",
    "codegen": "graphql-codegen -c ./codegen.yml",
    "export": "yarn load-env-dev npm run build && next export && npm run post-export",
    "post-export": "ts-node ./scripts/postexport.ts",
    "deploy": "npm run export && netlify deploy",
    "test": "jest",
    "ai-recipe-day-download": "ts-node ./scripts/downloadAIRecipe.ts",
    "load-env-dev": "dotenv -e .env.local",
    "ai-recipe-day": "npm run load-env-dev ansible-playbook deploys/publish-recipe-of-day.yml ",
    "ts": "ts-node"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@react-hooks-library/core": "^0.6.0",
    "@types/jest": "^29.2.0",
    "@types/lunr": "^2.3.4",
    "@vanilla-extract/babel-plugin": "^1.1.7",
    "@vanilla-extract/css": "^1.14.1",
    "@vanilla-extract/css-utils": "^0.1.2",
    "@vanilla-extract/next-plugin": "^2.1.1",
    "@vanilla-extract/sprinkles": "^1.4.1",
    "axios": "^0.27.2",
    "concurrently": "^7.4.0",
    "copy-to-clipboard": "^3.3.2",
    "env-cmd": "^10.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.2.2",
    "jimp": "^0.22.7",
    "jsonschema": "^1.4.1",
    "lunr": "^2.3.9",
    "mini-debounce": "^1.0.8",
    "next": "12.3.4",
    "next-plausible": "^3.7.2",
    "nodemailer": "^6.9.1",
    "pino": "^8.4.1",
    "pino-pretty": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.6.0",
    "react-popper": "^2.3.0",
    "serve": "^14.0.1",
    "sharp": "^0.32.0",
    "ssr-window": "^4.0.2",
    "striptags": "^3.2.0",
    "tinyduration": "^3.2.4"
  },
  "devDependencies": {
    "@graphql-cli/codegen": "^2.4.0",
    "@graphql-codegen/cli": "^2.16.2",
    "@graphql-codegen/typescript": "^2.8.6",
    "@swc/core": "^1.2.231",
    "@swc/helpers": "^0.4.6",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/node": "18.0.6",
    "@types/nodemailer": "^6.4.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.20.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "graphql": "^16.6.0",
    "graphql-cli": "^4.1.0",
    "lefthook": "^1.1.0",
    "minimatch": "^9.0.3",
    "prettier": "^2.7.1",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.55.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "5.0"
  }
}
